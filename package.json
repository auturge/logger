{
    "name": "@auturge/logger",
    "description": "A handy multiplexing logger.",
    "version": "1.0.3",
    "author": "auturge",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/auturge/logger/issues"
    },
    "homepage": "https://github.com/auturge/logger",
    "repository": {
        "type": "git",
        "url": "https://github.com/auturge/logger"
    },
    "engines": {
        "node": ">=12.8.1"
    },
    "main": "logger.js",
    "module": "logger.es6.js",
    "browser": "logger.umd.js",
    "devDependencies": {
        "@auturge/json-trim": "^1.0.0",
        "@auturge/testing": "^1.0.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@types/chai": "^4.2.18",
        "@types/mocha": "^8.2.2",
        "@types/sinon": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "@wessberg/rollup-plugin-ts": "^1.3.14",
        "awesome-typescript-loader": "^5.2.1",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.0",
        "coveralls": "^3.1.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "mocha": "^8.4.0",
        "mocha-lcov-reporter": "^1.3.0",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "rollup": "^2.47.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-ts-paths": "^1.0.5",
        "shx": "^0.3.3",
        "sinon": "^10.0.0",
        "source-map": "^0.7.3",
        "source-map-support": "^0.5.19",
        "tsconfig-paths": "^3.9.0",
        "ts-mocha": "^8.0.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4",
        "tspath": "^1.3.7"
    },
    "dependencies": {
        "tslib": "^2.2.0",
        "colorette": "^1.2.2",
        "date-fns": "^2.21.3",
        "date-fns-tz": "^1.1.4"
    },
    "scripts": {
        "build": "npm-run-all clean ~build:dist",
        "clean": "shx rm -rf dist build test/coverage",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "package": "npm-run-all clean ~mkdir:dist ~copy",
        "prepublishOnly": "node preventPublish.js",
        "test": "npm-run-all lint ~test:clean ~test:run",
        "tsc": "tsc -p ./tsconfig.json",
        "tsc:paths": "tsc && tspath -f",
        "~build:dist": "npm-run-all ~rollup ~copy:all",
        "~copy:all": "npm-run-all ~copy:readme ~copy:license ~copy:changelog ~copy:package:json",
        "~copy:changelog": "shx cp ./CHANGELOG.md ./dist",
        "~copy:license": "shx cp ./LICENSE ./dist",
        "~copy:package:json": "json-trim",
        "~copy:readme": "shx cp ./README.md ./dist",
        "~mkdir:dist": "shx mkdir ./dist",
        "~rollup": "rollup -c",
        "~test:clean": "shx rm -rf coverage",
        "~test:run": "nyc ts-mocha --paths -p ./tsconfig.json ./test/**/*.spec.ts",
        "~test:report": "nyc report --reporter=text-lcov | coveralls",
        "------------": "------------------------------------------------------------",
        "demo": "node ./test/objects/demo"
    }
}
